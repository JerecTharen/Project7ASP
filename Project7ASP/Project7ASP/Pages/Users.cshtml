@page
@model Project7ASP.Pages.UsersModel
@{
    ViewData["Title"] = "Users";
}

<h2>Users</h2>

<table>
    <thead>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Email</th>
    </thead>
        <tbody>
            @foreach (var user in Model.AllUsers)
            {
                <tr>
                    <td>@user.user_name</td>
                    <td>@user.first_name</td>
                    <td>@user.last_name</td>
                    <td>@user.age</td>
                    <td>@user.email</td>
                </tr>}
        </tbody>
</table>

<h2>Database Users</h2>
<table>
    <thead>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Email</th>
    </thead>
        <tbody>
            @foreach (var dbUser in Model.Users)
            {
                <tr>
                    <td>@dbUser.user_name</td>
                    <td>@dbUser.first_name</td>
                    <td>@dbUser.last_name</td>
                    <td>@dbUser.age</td>
                    <td>@dbUser.email</td>
                </tr>}
        </tbody>
</table>

